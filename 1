#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int N;
	cin >> N;
	int a[N];
	int count=0;
	for ( int i = 0 ; i < N; i++ ){
	    cin >> a[i];
	}
	for ( int i = 0 ; i < N; i++) {
	    if ( a[i] % 2 == 0) count++;
	}
	if (1) {
	    if ( count > N/2) {
	        cout << "ready for battle" << endl;
	    }else {
	        cout << "not ready" << endl;
	    }
	}
	
	return 0;
}

